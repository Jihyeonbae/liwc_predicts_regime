}

@article{fearon_bargaining_1998,
	title = {Bargaining, {Enforcement}, and {International} {Cooperation}},
	volume = {52},
	issn = {0020-8183, 1531-5088},
	url = {https://www.cambridge.org/core/product/identifier/S0020818398440360/type/journal_article},
	doi = {10.1162/002081898753162820},
	abstract = {Neoliberals and their neorealist critics have debated the relative importance of two main obstacles to international cooperation—problems of cheating and enforcement and problems of relative gains. By contrast, I argue that problems of international cooperation have a common strategic structure in which a third, distinct obstacle plays a crucial role. Almost regardless of the issue area, states must first resolve the
              bargaining problem
              of agreeing on terms before they can implement and begin to enforce an agreement. Furthermore, the bargaining and enforcement problems interact. Using a game model, I show that if states must bargain to determine the deal to be enforced, the “shadow of the future” cuts two ways. A high expectation of continued interactions may make enforcing the agreement easier, but it can also give states an incentive to bargain harder, delaying agreement in hopes of getting a better deal. Empirical evidence from trade and arms control negotiations suggests that this mechanism may help to explain the costly standoffs that are often observed in international politics and are problematic for received neoliberal theories.},
	language = {en},
	number = {2},
	urldate = {2022-04-04},
	journal = {International Organization},
	author = {Fearon, James D.},
	year = {1998},
	pages = {269--305},
}

